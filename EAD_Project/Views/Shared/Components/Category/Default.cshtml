@model List<string>
<style>
    .CategoryCard{
        display: inline-block;
        overflow-x: Scroll;
        margin: 10px;
    }

    .CategoryCard :hover{
            box-shadow: 0 0 1em #222;
    }
</style>
<div class="container-catagories text-center" style="margin-top: 10px" id="catagories">
    <div class="row gx-2">
        @foreach (string item in Model)
        {
            <partial name="_CatagoryCard" model="item"/>
        }
    </div>
    
</div>